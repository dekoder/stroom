<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:form="urn:import:stroom.widget.form.client"
             xmlns:v="urn:import:stroom.widget.valuespinner.client">
    <g:FormPanel ui:field="form" styleName="max">
        <g:FlowPanel styleName="max form">
            <form:FormGroup identity="stepLocationStreamId" label="Stream Id" addStyleNames="dock-min">
                <v:ValueSpinner ui:field="metaId" addStyleNames="w-100"/>
            </form:FormGroup>
            <form:FormGroup identity="stepLocationPartNumber" label="Part Number" addStyleNames="dock-min">
                <v:ValueSpinner ui:field="partNo" addStyleNames="w-100"/>
            </form:FormGroup>
            <form:FormGroup identity="stepLocationRecordNumber" label="Record Number" addStyleNames="dock-min">
                <v:ValueSpinner ui:field="recordNo" addStyleNames="w-100"/>
            </form:FormGroup>
        </g:FlowPanel>
    </g:FormPanel>
</ui:UiBinder>
