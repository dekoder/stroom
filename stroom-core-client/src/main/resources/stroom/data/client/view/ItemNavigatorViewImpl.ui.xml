<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:btn="urn:import:stroom.widget.button.client"
             xmlns:refresh-button="urn:import:stroom.data.pager.client">
    <g:FlowPanel styleName="itemNavigator">
        <g:Label ui:field="lblDetail" styleName="itemNavigator-label"/>
        <g:FlowPanel styleName="button-container">
            <btn:SvgButton ui:field="firstPageBtn"/>
            <btn:SvgButton ui:field="previousPageBtn"/>
            <btn:SvgButton ui:field="nextPageBtn"/>
            <btn:SvgButton ui:field="lastPageBtn"/>
            <refresh-button:RefreshButton ui:field="refresh"/>
        </g:FlowPanel>
    </g:FlowPanel>
</ui:UiBinder>
