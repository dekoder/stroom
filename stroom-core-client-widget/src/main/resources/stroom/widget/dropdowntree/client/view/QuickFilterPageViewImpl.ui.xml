<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:view="urn:import:stroom.widget.dropdowntree.client.view"
             xmlns:form="urn:import:stroom.widget.form.client">
    <g:FlowPanel styleName="max dock-container-vertical form-padding">
        <form:FormGroup ui:field="formGroup"
                        addStyleNames="form-label-padding">
            <view:QuickFilter ui:field="quickFilter"/>
        </form:FormGroup>

        <g:SimplePanel ui:field="data" styleName="dock-max overflow-hidden"/>
    </g:FlowPanel>
</ui:UiBinder>
